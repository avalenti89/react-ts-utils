{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"baseUrl": "./build/types",
		"paths": {
			"components/*": ["./build/types/components/*"],
			"components": ["./build/types/components"],
			"hooks/*": ["./build/types/hooks/*"],
			"hooks": ["./build/types/hooks"],
			"types/*": ["./build/types/types/*"],
			"types": ["./build/types/types"],
			"*": ["./build/types/*"],
			"": ["./build/types"]
		}
	}
}
